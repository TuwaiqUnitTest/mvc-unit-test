
@{
	var admin =(AdminModel) ViewData["admin"];
}
<table class="table table-hover" style="background-color: white; box-shadow: 5px 10px 18px #000000; ">
    <thead>
        <tr>
            <th scope="col" style="color: #0c4271; font-size:30px">Id</th>
            <th scope="col" style="color: #0c4271; font-size:30px">Name</th>
            <th scope="col" style="color: #0c4271; font-size:30px">Email</th>
            <th scope="col" style="color: #0c4271; font-size:30px">Phone Number</th>
        </tr>
    </thead>
         <tbody>
            <tr>
                <td style="color: #064420; font-size: 20px ">@admin.admin_Id </td>
                <td style="color: #064420; font-size: 20px ">@admin.admin_Name </td>
                <td style=" color: #064420;font-size:20px">@admin.admin_Email </td>
                <td style=" color: #064420;font-size:20px">@admin.admin_PhonNumber </td>

            </tr>
        </tbody>
</table>